<launch>
<arg name="frame" value="enu"/>

<node pkg="visibility_graph" type="visibility_graph_node" name="visibility_graph_node" output="screen">
    
    <param name="map/polygon_vertices_size" value="4"/>
    <param name="map/frame" value="$(arg frame)"/>
    <param name="map/change_start_end_hz" value="5"/>
    <rosparam param="map/height_limit"> [1.00, 5.00] </rosparam>

    <rosparam param="planning/start"> [0.0, -15.00, 5.00] </rosparam>
    <rosparam param="planning/end"> [0.0, 15.00, 1.00] </rosparam>

    <rosparam if="$(eval frame == 'nwu')" param="planning/obstacles"> 
        [-3.0, 4.00, -3.00, 8.00, -6.00, 8.00, -5.00, 4.00, 5.00,
        -3.0, 0.00, -3.00, -2.00, -5.00, 0.00, -4.00, -2.00, 6.00,
        -2.00, 1.00, -3.00, 1.00, -2.00, -1.00, -3.00, -1.00, 3.00,
        0.00, 0.00, 1.00, 0.00, 1.00, 2.00, 0.00, 2.00, 2.00,
        0.00, -2.00, 1.00, -2.00, 1.00, 0.00, 0.00, 0.00, 4.00,
        -1.00, -6.00, 4.00, -6.00, 4.00, -3.00, 2.00, -3.00, 5.00,
        -1.0, -8.00, 4.00, -8.00, 4.00, -6.00, -1.0, -6.00, 4.00,
        7.00, -6.00, 9.00, -6.00, 9.00, 2.00, 7.00, 2.00, 4.00] 
    </rosparam>
    <rosparam if="$(eval frame == 'enu')" param="planning/obstacles"> 
        [-4.0, -3.00, -8.00, -3.00, -8.00, -6.00, -4.00, -5.00, 5.00,
        0.0, -3.00, 2.00, -3.00, 0.00, -5.00, 2.00, -4.00, 6.00,
        -1.00, -2.00, -1.00, -3.00, 1.00, -2.00, 1.00, -3.00, 3.00,
        0.00, 0.00, 0.00, 1.00, -2.00, 1.00, -2.00, 0.00, 2.00,
        2.00, 0.00, 2.00, 1.00, 0.00, 1.00, 0.00, 0.00, 4.00,
        6.00, -1.00, 6.00, 4.00, 3.00, 4.00, 3.00, 2.00, 5.00,
        8.0, -1.00, 8.00, 4.00, 6.00, 4.00, 6.0, -1.00, 4.00,
        6.00, 7.00, 6.00, 9.00, -2.00, 9.00, -2.00, 7.00, 4.00] 
    </rosparam>

    <param name="planning/protected_zone" value="0.5"/>

</node>

<!-- Launch RViz with the demo configuration -->
<node if="$(eval frame == 'nwu')" name="rviz" pkg="rviz" type="rviz"
    args="-d $(find visibility_graph)/rviz/vis_graph_nwu.rviz"/>
<node if="$(eval frame == 'enu')" name="rviz" pkg="rviz" type="rviz"
    args="-d $(find visibility_graph)/rviz/vis_graph_enu.rviz"/>

</launch>